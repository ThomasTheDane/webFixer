{
  "name": "Paginate",
  "description": "Paginate several pages into one",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
        "matches": ["*://*/*"],
        "js": ["jquery-3.0.0.min.js", "content_script.js"],
        "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "Paginate"
  },
  "manifest_version": 2
}
